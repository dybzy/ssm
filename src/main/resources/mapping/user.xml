<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyb.dao.UserDao">
    <select id="selectByPrimaryKey" resultType="com.dyb.model.User">
       SELECT user_name 'userName',`password`,age FROM user_t where id=#{userId}
    </select>
</mapper>